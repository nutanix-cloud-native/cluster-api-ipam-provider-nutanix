{
    "$schema": "https://json-schema.org/draft/2020-12/schema",
    "properties": {
        "deployment": {
            "properties": {
                "replicas": {
                    "type": "integer"
                }
            },
            "type": "object"
        },
        "env": {
            "properties": {},
            "type": "object"
        },
        "image": {
            "properties": {
                "pullPolicy": {
                    "type": "string"
                },
                "repository": {
                    "type": "string"
                },
                "tag": {
                    "type": "string"
                }
            },
            "type": "object"
        },
        "imagePullSecrets": {
            "type": "array"
        },
        "nodeSelector": {
            "properties": {},
            "type": "object"
        },
